{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "assets/images/icon.png",
    "category": "customer"
  },
  "type": "REST",
  "lang": {
    "en-US": {
      "name": "CA-TEST",
      "description": "ca test"
    }
  },
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "EmailAddress": "{{InteractionDefaults.Email}}"
        },
        {
          "Phone": "{{Contact.Default.Phone}}"
        },
        {
          "FirstName": "{{Contact.Default.FirstName}}"
        },
        {
          "LastName": "{{Contact.Default.LastName}}"
        },
        {
          "SubscriberKey": "{{Contact.Default.SubscriberKey}}"
        }
      ],
      "outArguments": [
        {
          "foundSignupDate": ""
        }
      ],
      "url": "https://ca-sub-project.onrender.com/execute"
    }
  },
  "configurationArguments": {
    "save": {
      "url": "https://ca-sub-project.onrender.com/save"
    },
    "publish": {
      "url": "https://ca-sub-project.onrender.com/publish"
    },
    "validate": {
      "url": "https://ca-sub-project.onrender.com/validate"
    },
    "stop": {
      "url": "https://ca-sub-project.onrender.com/stop"
    }
  },
  "wizardSteps": [
    { "label": "메세지 타입설정", "key": "step1" },
    { "label": "메세지 설정", "key": "step2" },
    { "label": "완료", "key": "step3" }
  ],
  "userInterfaces": {
    "configModal": {
      "height": 200,
      "width": 300,
      "fullscreen": true
    }
  },
  "schema": {
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "SubscriberKey": {
              "dataType": "text",
              "isNullable": false,
              "direction": "in"
            }
          },
          {
            "FirstName": {
              "dataType": "text",
              "isNullable": true,
              "direction": "in"
            }
          },
          {
            "LastName": {
              "dataType": "text",
              "isNullable": true,
              "direction": "in"
            }
          },
          {
            "Phone": {
              "dataType": "text",
              "isNullable": true,
              "direction": "in"
            }
          },
          {
            "EmailAddress": {
              "dataType": "Email",
              "isNullable": true,
              "direction": "in"
            }
          }
        ],
        "outArguments": [
          {
            "foundSignupDate": {
              "dataType": "Date",
              "direction": "out",
              "access": "visible"
            }
          }
        ]
      }
    }
  }
}